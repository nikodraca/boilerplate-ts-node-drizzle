{
  "name": "boilerplate-ts-node-drizzle",
  "version": "1.0.0",
  "main": "index.ts",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "drizzle-orm": "0.44.4",
    "express": "5.1.0",
    "express-jwt": "^7.7.5",
    "express-openid-connect": "^2.8.0",
    "inversify": "6.0.1",
    "inversify-express-utils": "6.4.3",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "postgres": "^3.3.5",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "tsx": "^4.20.3",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/lodash": "^4.14.191",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.11.2",
    "@types/uuid": "^8.3.0",
    "drizzle-kit": "^0.19.13",
    "typescript": "~4.7.0"
  },
  "scripts": {
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx --tsconfig tsconfig.json db/migrate.ts",
    "dev": "NODE_ENV=development nodemon -L --exec 'tsx --tsconfig tsconfig.json' src/index.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node ./dist/index.js",
    "format": "prettier --write --config .prettierrc .",
    "format:check": "prettier --check --config .prettierrc ."
  }
